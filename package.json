  "scripts": {
    "start": "npm run generate-manifests && node scripts/start.js",
    "build": "npm run generate-manifests && node scripts/build.js && npm run build:prerender",
    "build:prerender": "react-snapshot --build-dir build-web",
    "build:desktop": "npm run get-electron-redists && npm run promote-content-to-executables",
    "build:with-desktop": "npm run build && npm run build-desktop",
    "build:subset-font": "node scripts/subsetFont.js",
    "build:debug": "npm run generate-manifests && cross-env NODE_ENV=development webpack --config config/webpack/webpack.config.js --verbose",
    "generate-manifests": "node scripts/generateManifests.js",
    "generate-manifest:passages": "node scripts/generatePassagesManifest.js",
    "generate-manifest:plugins": "node scripts/generatePluginsManifest.js",
    "generate-manifest:headers": "node scripts/generateHeadersManifest.js",
    "generate-manifest:footers": "node scripts/generateFootersManifest.js",
    "get-electron-redists": "node scripts/getElectronRedists.js",
    "promote-content-to-executables": "node scripts/promoteContentToExecutables.js",
    "test": "npm run generate-manifests && node scripts/test.js --config=config/testing/jest.config.js",
    "test:nowatch": "npm run test -- --dontWatch",
    "test:coverage": "npm run test -- --coverage",
    "prepublishOnly": "npm run test:nowatch"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
