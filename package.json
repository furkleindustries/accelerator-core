    "url": "https://github.com/furkleindustries/accelerator-core/issues"
  },
  "scripts": {
    "start": "npm run build:common && node -r esm scripts/start.js",
    "build": "rimraf build-web/ && npm run build:common && node -r esm scripts/build.js && npm run build:prerender",
    "build:common": "npm run generate-manifests && npm run build:download-fonts-and-subset",
    "build:prerender": "react-snap",
    "build:desktop": "npm run get-electron-redists && npm run promote-content-to-executables",
    "build:with-desktop": "npm run build && npm run build-desktop",
    "build:download-fonts-and-subset": "node -r esm scripts/downloadFontsAndSubset.js",
    "build:debug": "npm run generate-manifests && cross-env NODE_ENV=development node node_modules/webpack/bin/webpack.js -r esm --config config/webpack/webpack.config.js --verbose",
    "generate-manifests": "node -r esm scripts/generateManifests.js",
    "generate-manifest:passages": "node -r esm scripts/generatePassagesManifest.js",
    "generate-manifest:plugins": "node -r esm scripts/generatePluginsManifest.js",
    "generate-manifest:headers": "node -r esm scripts/generateHeadersManifest.js",
    "generate-manifest:footers": "node -r esm scripts/generateFootersManifest.js",
    "generate-manifest:story-options": "node -r esm scripts/generateStoryOptionsManifest.js",
    "get-electron-redists": "node -r esm scripts/getElectronRedists.js",
    "promote-content-to-executables": "node -r esm scripts/promoteContentToExecutables.js",
    "test": "npm run generate-manifests && node -r esm scripts/test.js --config=config/testing/jest.config.js",
    "test:nowatch": "npm run test -- --dontWatch",
    "test:coverage": "npm run test -- --coverage",
    "prepublishOnly": "npm shrinkwrap && npm run test:nowatch"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "source": "build-web"
  },
